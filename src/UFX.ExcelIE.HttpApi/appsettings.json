{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog",
      "Serilog.Exceptions",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Exceptionless",
      "UFX.Serilog.Enrichers"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "System": "Information",
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "restrictedToMinimumLevel": "Verbose"
              }
            },
            {
              "Name": "File",
              "Args": {
                "path": "Logs/.log",
                "rollingInterval": "Day",
                "restrictedToMinimumLevel": "Information",
                "formatter": "Serilog.Formatting.Json.JsonFormatter"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithThreadName",
      "WithProcessId",
      "WithProcessName",
      "WithEnvironmentUserName",
      "WithClientIp",
      "WithClientAgent",
      "WithTntId",
      "WithRequestHeaders",
      "WithRequestCookies",
      "WithRequestBodies",
      "WithResponseBodies",
      "WithExceptionDetails"
    ],
    "Properties": {
    }
  },
  //Redis
  "RedisConfig": {
    "DefaultDataBase": "0",
    "Connection": [ "127.0.0.1:6379" ],
    "Password": "",
    "IsOpenSentinel": false,
    "RedisSentinelIp": [],
    "ConnectTimeout": 300,
    "RedisPrefix": "common:"
  },
